-content_for :head do
  :javascript
    $(document).ready(function() {
      $('a[rel="popover"]').popover({
        placement:'top',
        html: true,
        
      });
    });
.container.archive
  .row
    .span10.offset1.survey-titles.center
      %h1.survey-title
        The Nutribox-Mini Survey
      %h2.survey-smaller-title
        Please help us by rating your Nutribox-mini for January 2012
    .span10.offset1    
      .row.survey-items   
         
        .survey-item.span10
          .well
            .survey-info
              .survey-image-wrap
                =image_tag "archive-image.jpg"
              .clearfix  
              .survey-item-content  
                %h2.survey-item-title
                  Raw Living Magic Mix
                %p.survey-item-desciption
                  Like a trail mix with a magic blend of goji berries, mulberries, yacon root, coconut chips, and carob chips. Sprinkle on cereal or yoghurt as well as eating on its own.
            .survey-right-col
              .survey-ratings
                %h3.survey-rate-title
                  Please rate this product
                %div
                  %span.rating
                    .yum-yuck
                      .yum
                        Yum
                      .yuck
                        Yuck
                    %span.star
                    %span.star
                    %span.star
                    %span.star
                    %span.star
              .clearfix      
              .survey-comments.mt-2
                =link_to "#", :class=>"btn-primary btn add-comment-button" do
                  %i.icon-comment
                  %span
                    Add a comment 
            .clearfix    
            
